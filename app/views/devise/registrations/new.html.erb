<!-- Sign In Option 1 -->
<div id="sign_up1">
    <div class="container">
        <div class="row">
            <div class="span12 header">
                <h4>Create your account</h4>
                <p>
                    There are many variations of passages of Lorem alteration in some form  injected humour these randomised words .</p>

					<%#= render partial: 'social' %>
					
	                <div class="span3 division">
	                    <div class="line l"></div>
	                    <span>or</span>
	                    <div class="line r"></div>
	                </div>
					
					<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
					  <%= devise_error_messages! %>
					

	                        
					  <h3>Warm-up</h3>
                     <div class="span12 footer">  <%= f.text_field :fullname, placeholder: "Full Name", type:"text"  %> </div><br><br>
                     <div class="span12 footer">  <%= f.email_field :email, :autofocus => true, placeholder: "Email", type:"text"  %></div><br><br>
				     <div class="span12 footer">  <%= f.password_field :password, placeholder: "Password", type:"password" %> </div><br><br>
                     <div class="span12 footer">  <%= f.password_field :password_confirmation, placeholder: " Confirm Password", type:"password" %> </div><br><br>
							<h3>Workout</h3>
	                  <div class="span12 footer">  <%= f.text_field :city, placeholder: "City", type:"text"  %> </div><br><br> 
					  <div class="span12 footer">  <%= f.text_field :state_abbreviation, placeholder: "2 digit state abbreviation", type:"text"  %> </div><br><br> 
					  <div class="span12 footer">  <%= f.text_field :phone_number, placeholder: "10 digit phone number", type:"text"  %> </div><br><br>       
	                        
	                        <div class="span12 footer">
							<%= f.submit "Sign up", type: "submit", value:"sign up" %>
	                        </div>

	                <!-- <div class="span5 remember">
	                    <label class="checkbox">
	                        <input type="checkbox"> Remember me
	                    </label>
	                    <a href="#">Forgot password?</a>
	                </div> -->
						
	                <div class="span12 dosnt">
	                    <span><br /><br />Already have an account?</span>
	                    <a href="sign-in.html">Sign in</a>
	                </div>
	            </div>
	        </div>
	    </div>



  
  
 
<% end %>

